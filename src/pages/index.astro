---
import Layout from "@layouts/Layout.astro";

// Keystatic content
import { createReader } from "@keystatic/core/reader";
import keystaticConfig from "@base/keystatic.config";
const reader = createReader("", keystaticConfig);
const page = await reader.singletons.home.read();
---

<Layout title={page?.title || "Beauty - Spa"}>
  <section>
    <h1>{page?.title}</h1>
    <p></p>
  </section>
</Layout>
